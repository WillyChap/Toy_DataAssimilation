    "Tx,Ty,Tz = LOeuler(fcstep,dt,xs,ys,zs,a,r,B)\n",
    "\n",
    "Tx = np.append(Tx,[Tx[-1]]);\n",
    "Ty = np.append(Ty,[Ty[-1]]);\n",
    "Tz = np.append(Tz,[Tz[-1]]);\n",
    "\n",
    "plt.plot(Tx,Tz)\n",
    "plt.xlabel('x')\n",
    "plt.ylabel('z')\n",
    "plt.title('Lorenz Butterfly')\n",
    "plt.grid(True)\n",
    "plt.show()"
